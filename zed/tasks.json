[
  {
    "label": "Example Python task",
    "command": "python3",
    "args": [".zed/example_py_task.py"],
    "use_new_terminal": true,
    "cwd": "$ZED_WORKTREE_ROOT"
  },
  {
    "label": "Run Blockd",
    "command": "go",
    "args": [
      "run",
      "${ZED_WORKTREE_ROOT}/blockd/cmd/blockd.go",
      "--url",
      "https://op-pokt.nodies.app",
      "--blockscout-url",
      "https://optimism.blockscout.com",
      "--blocks-back",
      "1000",
      "--queue-length",
      "10000",
      "--daemon"
    ],
    "env": {
      "key": "value"
    },
    "reveal": "always"
  }
]
